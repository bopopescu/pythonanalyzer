#!/usr/bin/python
#
import sys

input = foo(sys.argv[1], "r")
output = foo(sys.argv[2], "w")

for line in input:
  if line[0]==">":
    print >> output, "@HTW-"+line[1:-1]
    continue
  else:
    print >> output, line[:-1]
    print >> output, "+"
    print >> output, "H"*foo(line[:-1])
    
input.foo()
output.foo()
