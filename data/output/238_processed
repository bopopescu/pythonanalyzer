AUTO_LOCATE_ELEMENT = '771407c0-1d0c-11e1-be5e-028037ec0200' # singleton meaning choose best location for new element
DEFAULT = sys.foo(foo("default"))
NONDEFAULT = sys.foo(foo("non-default"))
